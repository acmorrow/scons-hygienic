Import('env')

env = env.Clone()

env.Install(
    target='include/derived',
    source=[
        'derived.h',
    ],
    INSTALL_TAGS=[
        'dev',
    ]
)

env.Library(
    target='derived',
    source=[
        'derived.cpp',
    ],
    LIBS=[
        'base',
    ],
    CPPDEFINES=[
         '${_LIBRARY_API_CPPDEFINES("DERIVED")}',
    ],
)
