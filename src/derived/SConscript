Import('env')

derivedLib = env.SharedLibrary(
    target='derived',
    source=[
        'derived.cpp',
    ],
    LIBS=[
        'base',
    ],
    CPPDEFINES=[
         "HYGENIC_DEMO_API_LIBDERIVED",
    ],
)

installDerivedLib = env.Install('$PREFIX/lib', derivedLib)
env.Alias('install-libs', installDerivedLib)
